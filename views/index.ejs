<%- include("header") -%>
    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>


    <div class="box">


        <% newItem.forEach((item)=>{ %>
            <div class="item">
                <input type="checkbox" name="" id="">
                    <p><%= item.name %></p>
                </div>

        <% }); %>

        <form  class="item" action="/" method="post">
            <input type="text" name="item" id="item" autocomplete="off" placeholder="Enter Item" />
            <button type="submit" name="list" value=<%=listTitle%>>+</button>
        </form>
    </div>
    <%- include("footer") -%>